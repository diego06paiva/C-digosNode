<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Lista de Livros</title>
  </head>
  <body>
    <h1>Lista de Livros</h1>
    <a href="/index">Página inicial</a>

    <ul>
      {{#each livros}}
      <div>
        #{{this.id}} - 
             <span class="bold"><a href="/dados/{{this.id}}">{{this.titulos}}</a></span>   
         <li> {{this.paginas}} páginas</li>
      {{/each}}
      </div>
    </ul>
  </body>
</html>